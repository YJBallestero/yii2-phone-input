{
    "name": "yjballestero/yii2-phone-input",
    "description": "Yii2 International telephone numbers - Asset Bundle, Behavior, Validator, Widget",
    "keywords": [
        "yii2",
        "intl-tel-input",
        "international telephone numbers",
        "libphonenumber"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "type": "yii2-extension",
    "homepage": "https://github.com/YJBallestero/yii2-phone-input",
    "support": {
        "issues": "https://github.com/YJBallestero/yii2-phone-input/issues?state=open",
        "source": "https://github.com/YJBallestero/yii2-phone-input"
    },
    "authors": [
        {
            "name": "Borales (Alexandr Bordun)",
            "email": "bordun.alexandr@gmail.com"
        },
        {
            "name": "Yanis Ballestero",
            "email": "yjballestero@gmail.com",
            "role": "Developer",
            "homepage": "https://yjballestero.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "yjballestero\\phoneInput\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yjballestero\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "giggsey/libphonenumber-for-php": "^8.0@dev",
        "yiisoft/yii2": "~2.0.45",
        "jackocnr/intl-tel-input": "~23.0.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
